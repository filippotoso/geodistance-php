        "geodistance",
        "gps",
        "distance",
        "harvesine"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "diegomariani",
            "email": "diego.mariani@mail.com"
        },
        {
            "name": "Filippo Toso",
            "email": "filippo@toso.dev"
        }
    ],
    "autoload": {
        "psr-4": {
